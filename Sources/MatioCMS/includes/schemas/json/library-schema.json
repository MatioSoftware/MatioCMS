{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "properties": {
        "Styles": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "Name": { "type": "string" },
                    "Paths": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "format": "uri"
                        },
                        "minLength": 1
                    }
                },
                "required": ["Name","Paths"]
            }
        },
        "Scripts": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "Name": { "type": "string" },
                    "Paths": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "format": "uri"
                        },
                        "minLength": 1
                    }
                },
                "required": ["Name","Paths"]
            }
        }
    },
    "required": ["Styles","Scripts"]
}
